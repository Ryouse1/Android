{
  "name": "Android-AOSP-Max",
  "image": "mcr.microsoft.com/vscode/devcontainers/base:ubuntu",
  "remoteUser": "vscode",
  "containerUser": "vscode",
  "runArgs": ["--privileged"],
  "postCreateCommand": "./scripts/install_repo.sh",
  "workspaceMount": "source=${localWorkspaceFolder},target=/workspaces/android,type=bind",
  "mounts": [
    "source=/mnt/aosp-data,target=/workspaces/aosp-data,type=bind"
  ],
  "machine": {
    "type": "standard-32"
  }
}
